<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Model"
                   namespace="Model">

  <class name="Administrateur" table="administrateur">
    <id name="Id" column="administrateur_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Login" column="administrateur_login" not-null="true"/>
    <property name="Mdp" column="administrateur_mdp" not-null="true"/>
  </class>

</hibernate-mapping>